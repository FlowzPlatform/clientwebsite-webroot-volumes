<div class="product-section-box">
    <div class="panel-group" id="product_description" role="tablist" aria-multiselectable="true">
        <div class="panel-heading section-open" role="tab">
            <h2 class="panel-title heading-block">Description</h2><a class="js-add-class tab-quantity collapsed" data-toggle="collapse"
            data-parent="#product_description" href="#product_description_block" aria-expanded="true"><i class="caret-icon"></i></a>
        </div>
        <div class="panel-collapse collapse in" id="product_description_block"
        role="tabpanel">
            <div class="pro-detail-col product_description_text"></div>
        </div>
    </div>
</div>
<div class="product-section-box production-shipping">
    <div class="panel-group" id="product_details" role="tablist" aria-multiselectable="true">
        <div class="panel-heading" role="tab">
            <h2 class="panel-title heading-block">Specifications</h2><a class="js-add-class tab-quantity collapsed" data-toggle="collapse"
            data-parent="#product_details" href="#product_details_block" aria-expanded="true"><i class="caret-icon"></i></a>
        </div>
        <div class="panel-collapse collapse" id="product_details_block" role="tabpanel">
            <div class="pro-detail-col" id="product-detail">
                <div class="inner-tab-content">
                    <table class="main-table" width="100%">
                        <tr>
                            <td>
                                <table class="product-detail-text sub-table border-right-10" width="100%"
                                cellpadding="5">
                                    <tr class="row">
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                    </tr>
                                    <tr class="row">
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="product-section-box production-shipping">
    <div class="panel-group" id="Imprint_Pricing" role="tablist" aria-multiselectable="true">
        <div class="panel-heading" role="tab">
            <h2 class="panel-title heading-block">Imprint Info</h2><a class="js-add-class tab-quantity collapsed" data-toggle="collapse"
            data-parent="#Imprint_Pricing" href="#Imprint_Pricing_block" aria-expanded="true"><i class="caret-icon"></i></a>
        </div>
        <div class="panel-collapse collapse" id="Imprint_Pricing_block" role="tabpanel">
            <div class="pro-detail-col" id="imprint-info">
                <div class="inner-tab-content">
                    <table class="main-table" width="100%">
                        <tr>
                            <td>
                                <table class="sub-table border-right-10 imprint-info-text" width="100%"
                                cellpadding="5">
                                    <tr class="row">
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                    </tr>
                                    <tr class="row">
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                        <td class="cell"></td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>